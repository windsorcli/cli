{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch Package",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "program": "${fileDirname}"
      },
      {
          "name": "Windsor Env",
          "type": "go",
          "request": "launch",
          "mode": "auto",
          "program": "${workspaceFolder}/cmd/windsor/main.go",
          "args": ["env"]
      },
      {
        "name": "Windsor Init",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/cmd/windsor/main.go",
        "args": ["init", "local", "--git-livereload"]
    },
  ]
}
